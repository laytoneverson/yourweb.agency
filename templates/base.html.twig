<!DOCTYPE html>

{% block preHead %}
<!--[if IE 8]> <html lang="en" class="ie8 no-js"> <![endif]-->
<!--[if IE 9]> <html lang="en" class="ie9 no-js"> <![endif]-->
<!--[if !IE]><!-->
<html lang="en">
<!--<![endif]-->
{% endblock preHead %}
    <head>
        <script async src="https://www.googletagmanager.com/gtag/js?id=UA-113142152-1"></script>
        <script>
          window.dataLayer = window.dataLayer || [];
          function gtag(){dataLayer.push(arguments);}
          gtag('js', new Date());
          gtag('config', 'UA-113142152-1');
        </script>

        <meta charset="utf-8">
        <meta name="maValidation" content="5bce1d9b4ed9487bbe086f1d319e128c" />
        <meta name="viewport" content="width=device-width, initial-scale=1.0">
        <meta http-equiv="X-UA-Compatible" content="IE=edge,chrome=1">
        <meta  name="author" content="YourWeb.online, Layton Everson <layton.everson@gmail.com>">
        <meta property="og:site_name" content="{% block socialSiteName %}Everything CryptoCurrency by YourWeb{% endblock %}">
        <meta property="og:image" content="{% block socialImageUrl %}http://cryptocurrency-db.oss-us-east-1.aliyuncs.com/LogoSquare.png{% endblock %}"><!-- link to image for socio -->
        <meta property="og:type" content="website">

        {% block socialMetaData %}
        <title>{% block title %}Cryptocurrency Resources by YourWeb!{% endblock %}</title>
        <meta name="description" content="{% block metaDescription %}Welcome to the CryptoCurrency by YourWeb Agency. Find the best bitcoin services in one place.{% endblock %}">
        <meta name="keywords" content="{% block metaKeywords %}Bitcoin, AltCoins, bitcoin services, best bitcoin wallet, best bitcoin exchange best bitcoin faucets, top paying faucets, altcoins{% endblock %}">
        <meta property="og:title" content="{% block socialTitle %}Everything CryptoCurrency By YourWeb.online{% endblock %}">
        <meta property="og:description" content="{% block socialDescription %}CryptoCurrency by YourWeb Agency. Find the best bitcoin services in one place.{% endblock %}">
        <meta property="og:url" content="{% block socialUrl %}https://cryptocurrency.yourweb.online{% endblock %}">
        {% endblock socialMetaData %}

        {#{% block manifest %}<script src="{{ asset('/build/manifest.js') }}"></script>{% endblock %}#}
        {% block fontsLink %}
            <link href="http://fonts.googleapis.com/css?family=Open+Sans:300,400,600,700|PT+Sans+Narrow|Source+Sans+Pro:200,300,400,600,700,900&amp;subset=all" rel="stylesheet" type="text/css">
        {% endblock %}
        {% block globalCssHref %}
            <link rel="stylesheet" href="{{ asset('/assets/plugins/font-awesome/css/font-awesome.min.css') }}">
            <link rel="stylesheet" href="{{ asset('/assets/plugins/bootstrap/css/bootstrap.min.css') }}">
            <link rel="stylesheet" href="{{ asset('build/global/style.css') }}">
            <link rel="stylesheet" href="{{ asset('build/global/app.css') }}">
            <link rel="stylesheet" href="{{ asset('build/global/global-custom.css') }}">
        {% endblock %}
        {% block themeStyleSheets %}
            <link href="{{ asset('/assets/pages/css/animate.css') }}" rel="stylesheet">
            <link href="{{ asset('/assets/corporate/css/style-responsive.css') }}" rel="stylesheet">
            <link href="{{ asset('/assets/plugins/fancybox/source/jquery.fancybox.css') }}" rel="stylesheet">
        {% endblock %}
        {% block pageCssHref %}{% endblock %}
        {% include 'includes/favicon.html.twig' %}
    </head>
    <body class="{% block bodyClass %} corporate {% endblock %}">
        {% include 'includes/facebook-sdk-include.html.twig' %}

        {% block preHeader %}{% endblock preHeader%}
        {% block topBar %}{% endblock topBar %}
        {% block pageHeader %}
        <div class="header">
            <div class="container">
                {% block siteLogo %}
                    <a class="site-logo" href="{% block logoLinkUrl %} https://CryptoCurrency.yourweb.online/ {% endblock %}">
                        <img src="{% block logoImageSrc %}/assets/logo/logo-small-dark.png{% endblock %}"
                             alt="{% block logoAltText %}CryptoCurrency By YourWeb.Online Logo{% endblock %}"
                             style="height: 55px">
                    </a>
                {% endblock siteLogo %}

                <a href="javascript:void(0);" class="mobi-toggler"><i class="fa fa-bars"></i></a>

                {% block siteNavigation %}
                    <div class="header-navigation font-transform-inherit">
                        <ul>

                            {% block headerNav %}
                                {% include 'nav.html.twig' %}
                            {% endblock %}

                            {% block search %}
                            <li class="menu-search">
                                <a href="https://twitter.com/intent/tweet?screen_name=BuildingYourWeb&ref_src=twsrc%5Etfw" class="twitter-mention-button" data-text="Hi, I have feedback for you: " data-related="BuildingYourWeb" data-show-count="false">Contact Us: @BuildingYourWeb</a><script async src="https://platform.twitter.com/widgets.js" charset="utf-8"></script>
                            </li>
                            {#<li class="menu-search">#}
                                {#<span class="sep"></span>#}
                                {#<i class="fa fa-search search-btn"></i>#}
                                {#<div class="search-box">#}
                                    {#<form action="#">#}
                                        {#<div class="input-group">#}
                                            {#<input type="text" placeholder="Search" class="form-control">#}
                                            {#<span class="input-group-btn">#}
                                                {#<button class="btn btn-primary" type="submit">Search</button>#}
                                            {#</span>#}
                                        {#</div>#}
                                    {#</form>#}
                                {#</div>#}
                            {#</li>#}
                            {% endblock search %}

                        </ul>
                    </div>
                {% endblock siteNavigation %}

            </div>
        </div>
        {% endblock pageHeader %}
        {% block bodyHero %}{% endblock bodyHero %}
        {% block bodyContent %}
        <div class="main">
            <div class="container">
            </div>
        </div>
        {% endblock bodyContent %}
        {% block pageBigFoot %}{% include 'big-foot.html.twig' %}{% endblock pageBigFoot %}
        {% block pageFinalFoot %}{% include 'final-foot.html.twig' %}{% endblock pageFinalFoot %}
        {% block globalJavascripts %}
            {#<script src="{{ asset('build/global/metronic.js') }}"></script>#}
            <script src="{{ asset('build/js/app.js') }}"></script>
            <script src="{{ asset('build/js/owl-carousel.js') }}"></script>
            <script src="{{ asset('/assets/plugins/jquery.min.js') }}" type="text/javascript"></script>
            <script src="{{ asset('/assets/plugins/jquery-migrate.min.js') }}" type="text/javascript"></script>
            <script src="{{ asset('/assets/plugins/bootstrap/js/bootstrap.min.js') }}" type="text/javascript"></script>
            <script src="{{ asset('build/global/back-to-top.js') }}" type="text/javascript"></script>
            <script src="{{ asset('build/global/app-gobal.js') }}" type="text/javascript"></script>
            {#<script src="{{ asset('build/global/layout.js') }}" type="text/javascript"></script>#}
            <!--[if lt IE 9]>
                <script src="{{ asset('/assets/plugins/respond.min.js') }}" type="text/javascript"></script>
            <![endif]-->
            <script type="text/javascript">
                // jQuery(document).ready(function() {
                //     Layout.init();
                //     Layout.initTwitter();
                //     Layout.initFixHeaderWithPreHeader(); /* Switch On Header Fixing (only if you have pre-header) */
                //     Layout.initNavScrolling();
                // });
            </script>
        {% endblock %}
        {% block pageJavascripts %}{% endblock %}
    </body>
</html>
