{% endblock preHead %}
{% block googleAnalyticsCode %}UA-113142152-1{% endblock %}

{% block title %}Everything CryptoCurrency!{% endblock %}
{% block metaDescription %}
Welcome to the CryptoCurrency by YourWeb Agency. Find the best bitcoin services in one place.
{% endblock %}
{% block metaKeywords %}Bitcoin, AltCoins, bitcoin services, best bitcoin wallet, best bitcoin exchange,
best bitcoin faucets, top paying faucets, altcoins{% endblock %}
{% block socialSiteName %}Everyting CryptoCurrency by YourWeb{% endblock %}
{% block socialTitle %}Everything CryptoCurrency By YourWeb.online{% endblock %}
{% block socialDescription %}CryptoCurrency by YourWeb Agency. Find the best bitcoin services in one place.{% endblock %}
{% block socialImageUrl %}https://scontent-lax3-1.xx.fbcdn.net/v/t1.0-1/c0.0.200.200/p200x200/26815110_182160825866115_4596465814440902108_n.png?oh=ee320467d86b0f1e198d7299d46a94a0&oe=5ADB3338{% endblock %}
{% block socialUrl %}https://www.facebook.com/YourWebPresentsCryptoCurrenc{% endblock %}
{% block favicon %}https://CryptoCurrency.yourweb.online/favicon.ico{% endblock %}
{% block manifest %}<script src="{{ asset('/build/manifest.js') }}"></script>{% endblock %}
{% block fontsLink %}
    <link href="http://fonts.googleapis.com/css?family=Open+Sans:300,400,600,700|PT+Sans+Narrow|Source+Sans+Pro:200,300,400,600,700,900&amp;subset=all" rel="stylesheet" type="text/css">
{% endblock %}
{% block globalCssHref %}
    <link rel="stylesheet" href="{{ asset('/assets/plugins/font-awesome/css/font-awesome.min.css') }}">
    <link rel="stylesheet" href="{{ asset('/assets/plugins/bootstrap/css/bootstrap.min.css') }}">
    <link rel="stylesheet" href="{{ asset('/build/global/app-global.css') }}">
{% endblock %}
{% block themeStyleSheets %}
    <link href="{{ asset('/assets/pages/css/animate.css') }}" rel="stylesheet">
    <link href="{{ asset('/assets/corporate/css/style.css') }}" rel="stylesheet">
    <link href="{{ asset('/assets/corporate/css/style-responsive.css') }}" rel="stylesheet">
    <link href="{{ asset('/assets/corporate/css/themes/green.css') }}" rel="stylesheet" id="style-color">

    <link href="{{ asset('/assets/plugins/fancybox/source/jquery.fancybox.css') }}" rel="stylesheet">

{% endblock %}

{% block pageCssHref %}{{ parent() }}{% endblock %}

{# Body BEGIN #}
{% block bodyClass %} corporate {% endblock %}

{% block preHeader %}{% endblock preHeader %}

{% block topBar %}
<div class="pre-header">
    <div class="container">
        <div class="row">

            {% block topBarLeft %}
            <div class="col-md-6 col-sm-6 additional-shop-info">
                <ul class="list-unstyled list-inline">

                    {% block topBarLeftItems %}
                    <li>
                        <i class="fa fa-twitter-square"></i>
                        <a href="" class="">Follow @laytone</a>
                    </li>
                    <li><i class="fa fa-envelope-o"></i><span>info@yourweb.online</span></li>
                    {% endblock topBarLeftItems %}
                </ul>
            </div>
            {% endblock topBarLeft %}

            {% block topBarRight %}
            <div class="col-md-6 col-sm-6 additional-nav">
                <ul class="list-unstyled list-inline pull-right">
                    {% block topBarRightItems %}
                    {# <li><a href="page-login.html">Log In</a></li> #}
                    {# <li><a href="page-reg-page.html">Registration</a></li> #}
                    {% endblock topBarRightItems %}
                </ul>
            </div>
            {% endblock topBarRight %}

        </div>
    </div>
</div>
{% endblock topBar %}

{% block pageHeader %}
<div class="header">
    <div class="container">

        {% block siteLogo %}
        <a class="site-logo"
          href="{% block logoLinkUrl %} https://CryptoCurrency.yourweb.online/ {% endblock %}"
          style="padding: 12px 0">
            <img src="{% block logoImageSrc %}/assets/logo/logo-small-dark.png{% endblock %}"
                 alt="{% block logoAltText %}CryptoCurrency By YourWeb.Online Logo{% endblock %}"
                 style="height: 55px">
        </a>
        {% endblock siteLogo %}

        <a href="javascript:void(0);" class="mobi-toggler"><i class="fa fa-bars"></i></a>

        {% block siteNavigation %}
        <div class="header-navigation pull-right font-transform-inherit">
            <ul>

                {% block headerNav %}
                    {% include 'cryptocurrency/nav-coins.html.twig' %}
                {% endblock %}

                {% block search %}
                <li class="menu-search">
                    <span class="sep"></span>
                    <i class="fa fa-search search-btn"></i>
                    <div class="search-box">
                        <form action="#">
                            <div class="input-group">
                                <input type="text" placeholder="Search" class="form-control">
                                <span class="input-group-btn">
                                    <button class="btn btn-primary" type="submit">Search</button>
                                </span>
                            </div>
                        </form>
                    </div>
                </li>
                {% endblock search %}

            </ul>
        </div>
        {% endblock siteNavigation %}

    </div>
</div>
{% endblock pageHeader %}

{% block bodyHero %}{{ parent() }}{% endblock bodyHero %}

{% block bodyContent %}
<div class="main">
    <div class="container">
    </div>
</div>
{% endblock bodyContent %}

{% block pageBigFoot %}{% include 'cryptocurrency/big-foot.html.twig' %}{% endblock pageBigFoot %}
{% block pageFinalFoot %}{% include 'cryptocurrency/final-foot.html.twig' %}{% endblock pageFinalFoot %}

{% block globalJavascripts %}
    <script src="{{ asset('/build/global/app-global.js') }}"></script>
    <script src="{{ asset('/build/js/app.js') }}"></script>
    <script src="{{ asset('/assets/plugins/jquery.min.js') }}" type="text/javascript"></script>
    <script src="{{ asset('/assets/plugins/jquery-migrate.min.js') }}" type="text/javascript"></script>
    <script src="{{ asset('/assets/plugins/bootstrap/js/bootstrap.min.js') }}" type="text/javascript"></script>
    <script src="{{ asset('/assets/corporate/scripts/back-to-top.js') }}" type="text/javascript"></script>

    <!--[if lt IE 9]>
        <script src="{{ asset('/assets/plugins/respond.min.js') }}" type="text/javascript"></script>
    <![endif]-->
    <!-- END CORE PLUGINS -->
{% endblock %}

{% block pageJavascripts %}{{ parent() }}{% endblock %}
