{% block preHead %}
<!--[if IE 8]> <html lang="en" class="ie8 no-js"> <![endif]-->
<!--[if IE 9]> <html lang="en" class="ie9 no-js"> <![endif]-->
<!--[if !IE]><!-->
<html lang="en">
<!--<![endif]-->
{% endblock preHead %}

{% block title %}Welcome to YourWeb.Agency!{% endblock %}
{% block metaDescription %}Web technology and resources. Php Development, CryptoCurrency, Docker{% endblock %}
{% block metaKeywords %}Php Development, CryptoCurrency, Docker{% endblock %}
{% block socialSiteName %}YourWeb.Agency{% endblock %}
{% block socialTitle %}YourWeb.Agency - Making the Web YourWeb.{% endblock %}
{% block socialDescription %}Talored web applications, resources for developers, and cryptocurrency. We're making the web YourWeb.{% endblock %}
{% block socialImageUrl %}{% endblock %}
{% block socialUrl %}http://yourweb.agency/{% endblock %}
{% block favicon %}favicon.ico{% endblock %}
{% block manifest %}<script src="{{ asset('build/manifest.js') }}"></script>{% endblock %}
{% block fontsLink %}
    <link href="http://fonts.googleapis.com/css?family=Open+Sans:300,400,600,700|PT+Sans+Narrow|Source+Sans+Pro:200,300,400,600,700,900&amp;subset=all" rel="stylesheet" type="text/css">
{% endblock %}
{% block globalCssHref %}
    <link href="assets/plugins/font-awesome/css/font-awesome.min.css" rel="stylesheet">
    <link href="assets/plugins/bootstrap/css/bootstrap.min.css" rel="stylesheet">
    <link rel="stylesheet" href="{{ asset('build/global/app-global.css') }}">
{% endblock %}
{% block themeStyleSheets %}
    <link href="assets/pages/css/animate.css" rel="stylesheet">
    <link href="assets/plugins/fancybox/source/jquery.fancybox.css" rel="stylesheet">
    <link href="assets/plugins/owl.carousel/assets/owl.carousel.css" rel="stylesheet">
    <link rel="stylesheet" href="{{ asset('build/theme/matronic.css') }}">
{% endblock %} 

{% block pageCssHref %}{{ parent() }}{% endblock %}

{# Body BEGIN #}
{% block bodyClass %} corporate {% endblock %}

{% block preHeader %}
    {% block styleCustomizer %}
    <div class="color-panel hidden-sm">
        <div class="color-mode-icons icon-color"></div>
        <div class="color-mode-icons icon-color-close"></div>
        <div class="color-mode">
            <p>THEME COLOR</p>
            <ul class="inline">
                <li class="color-red {% block themColorRed %}current{% endblock %} color-default" data-style="red"></li>
                <li class="color-blue" {% block themeColorBlue %} {% endblock %} data-style="blue"></li>
                <li class="color-green" {% block themeColorGree %} {% endblock %} data-style="green"></li>
                <li class="color-orange" {% block themeColorOrange %} {% endblock %} data-style="orange"></li>
                <li class="color-gray" {% block themeColorGray %} {% endblock %} data-style="gray"></li>
                <li class="color-turquoise" {% block themeColorTurquoise %} {% endblock %} data-style="turquoise"></li>
            </ul>
        </div>
    </div>
    {% endblock styleCustomizer %}
{% endblock preHeader %}

{% block topBar %}
<div class="pre-header">
    <div class="container">
        <div class="row">

            {% block topBarLeft %}
            <div class="col-md-6 col-sm-6 additional-shop-info">
                <ul class="list-unstyled list-inline">

                    {% block topBarLeftItems %}
                    <li>
                        <i class="fa fa-twitter-square"></i>
                        <a href="" class="">Follow @laytone</a>
                    </li>
                    <li><i class="fa fa-envelope-o"></i><span>info@yourweb.agency</span></li>
                    {% endblock topBarLeftItems %}

                </ul>
            </div>
            {% endblock topBarLeft %}

            {% block topBarRight %}
            <div class="col-md-6 col-sm-6 additional-nav">
                <ul class="list-unstyled list-inline pull-right">
                    
                    {% block topBarRightItems %}
                    <li><a href="page-login.html">Log In</a></li>
                    <li><a href="page-reg-page.html">Registration</a></li>
                    {% endblock topBarRightItems %}

                </ul>
            </div>
            {% endblock topBarRight %}

        </div>
    </div>
</div>
{% endblock topBar %}

{% block pageHeader %}
<div class="header">
    <div class="container">

        {% block siteLogo %}
        <a class="site-logo" 
          href="{% block logoLinkUrl %} http://yourweb.agency/ {% endblock %}"
        >
            <img src="{% block logoImageSrc %}assets/corporate/img/logos/logo-corp-red.png{% endblock %}"
                 alt="{% block logoAltText %}YourWeb.Agency Logo{% endblock %}">
        </a>
        {% endblock siteLogo %}

        <a href="javascript:void(0);" class="mobi-toggler"><i class="fa fa-bars"></i></a>

        {% block siteNavigation %}
        <div class="header-navigation pull-right font-transform-inherit">
            <ul>

                {% block headerNav %}
                <li class="dropdown">
                    <a class="dropdown-toggle" data-toggle="dropdown" data-target="#" href="javascript:">YourWeb</a>

                    <ul class="dropdown-menu">
                        <li><a href="http://yourweb.agency/">YourWeb Home</a></li>
                        <li class="active"><a href="http://digital-cash.yourweb.agency/">Digital Cash Home</a></li>
                    </ul>
                </li>
                <li><a href="/wallets">Wallets</a></li>
                <li><a href="/top-bitcoin-faucets">Best Faucets</a></li>
                <li><a href="/games-and-casinos">Games And Casions</a></li>
                <li><a href="/where-to-buy-bitcoins">Buy Coins</a></li>
                <li><a href="/invest-and-trade">Invest And Exchange</a></li>
                <li><a href="/mining">Mine Coins</a></li>
                {% endblock %}

                {% block search %}
                <li class="menu-search">
                    <span class="sep"></span>
                    <i class="fa fa-search search-btn"></i>
                    <div class="search-box">
                        <form action="#">
                            <div class="input-group">
                                <input type="text" placeholder="Search" class="form-control">
                                <span class="input-group-btn">
                                    <button class="btn btn-primary" type="submit">Search</button>
                                </span>
                            </div>
                        </form>
                    </div>
                </li>
                {% endblock search %}
                <!-- END TOP SEARCH -->
            </ul>
        </div>
        {% endblock siteNavigation %}

    </div>
</div>
{% endblock pageHeader %}

{% block bodyHero %}{{ parent() }}{% endblock bodyHero %}

{% block bodyContent %}
<div class="main">
    <div class="container">
    </div>
</div>
{% endblock bodyContent %}

{% block pageBigFoot %}{% include 'cryptocurrency/big-foot.html.twig' %}{% endblock pageBigFoot %}


{% block pageFinalFoot %}{% include 'cryptocurrency/final-foot.html.twig' %}{% endblock pageFinalFoot %}


{% block globalJavascripts %}
    <script src="{{ asset('build/global/app-global.js') }}"></script>
    <script src="{{ asset('build/js/app.js') }}"></script>
    <script src="assets/plugins/jquery.min.js" type="text/javascript"></script>
    <script src="assets/plugins/jquery-migrate.min.js" type="text/javascript"></script>
    <script src="assets/plugins/bootstrap/js/bootstrap.min.js" type="text/javascript"></script>      
    <script src="assets/corporate/scripts/back-to-top.js" type="text/javascript"></script>
    
    <!--[if lt IE 9]>
        <script src="assets/plugins/respond.min.js"></script>
    <![endif]-->
    <!-- END CORE PLUGINS -->
{% endblock %}

{% block pageJavascripts %}{{ parent() }}{% endblock %}
