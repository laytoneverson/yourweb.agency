{% extends 'base.html.twig' %}
{% import 'macros.html.twig' as macros %}

{% block bodyContent %}
    <main class="main">
        <div class="container">
            {% block breadCrumb %}
                <nav class="breadcrumb">
                    <li><a href="/">Home</a></li>
                    {% block breadCrumbItems %}
                        <li><a href="javascript:;">Service Index</a></li>
                        <li class="active">{% block breadcrumbEntry %}{% endblock breadcrumbEntry %}</li>
                    {% endblock breadCrumbItems %}
                </nav>
            {% endblock breadCrumb %}

            <div class="row margin-bottom-40">
                <div class="col-md-12 col-sm-12">
                    <article class="content-page">
                        <header>
                            <h1 class="pageTitle">
                                {% block indexPageTitle %}ADD ME block indexPageTitle{% endblock %}
                            </h1>
                            <p class="section-summary col-md-5">
                                {% block sectionSummary %}{% endblock sectionSummary %}
                            </p>
                            <p class="section-summary col-md-5">
                                {% block sectionSummaryRight %}{% endblock sectionSummaryRight %}
                            </p>
                        </header>
                        <div class="row">
                            <section class="col-sm-9 col-xs-12">
                                {% block indexBody %}

                                {% endblock %}
                            </section>
                            <div class="col-sm-3 hidden-xs-down">
                                {% block sideBar %}

                                {% endblock %}
                            </div>
                        </div>
                    </article>
                </div>
                <div class="row">
                    <div class="col-md-4">{{ macros.displaySquare() }}</div>
                    <div class="col-md-4">{{ macros.displaySquare() }}</div>
                    <div class="col-md-4">{{ macros.displaySquare() }}</div><
                </div>
            </div>
        </div>
    </main>
{% endblock %}
